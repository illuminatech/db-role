{
    "name": "illuminatech/db-role",
    "description": "Provides support for Eloquent relation role (table inheritance) composition in Laravel",
    "keywords": ["laravel", "eloquent", "role", "relation", "table", "inheritance"],
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/illuminatech/db-role/issues",
        "wiki": "https://github.com/illuminatech/db-role/wiki",
        "source": "https://github.com/illuminatech/db-role"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        }
    ],
    "require": {
        "illuminate/database": "^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0"
    },
    "require-dev": {
        "illuminate/events": "*",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.3 || ^10.0.7"
    },
    "autoload": {
        "psr-4": {
            "Illuminatech\\DbRole\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminatech\\DbRole\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}